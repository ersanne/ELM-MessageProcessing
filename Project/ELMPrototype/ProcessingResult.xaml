<Window x:Class="ELMPrototype.ProcessingResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ELMPrototype"
        mc:Ignorable="d"
        Title="File import result" Height="476" Width="489" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="ResultTextBox" HorizontalAlignment="Left" Height="369" Margin="10,40,0,0" TextWrapping="Wrap"
                 VerticalAlignment="Top" Width="461" Grid.ColumnSpan="2" />
        <Label x:Name="ResultLabel" Content="Result" HorizontalAlignment="Left" Margin="10,14,0,0"
               VerticalAlignment="Top" />
        <Button x:Name="SaveToFileBtn" Content="Save result" HorizontalAlignment="Left" Margin="300,414,0,0" VerticalAlignment="Top" Width="80" Click="SaveToFileBtn_Click"/>
        <Button x:Name="CloseBtn" Content="Close" HorizontalAlignment="Left" Margin="396,414,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.28,0.29" Click="CloseBtn_Click"/>
    </Grid>
</Window>